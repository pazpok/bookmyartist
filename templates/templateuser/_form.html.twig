{{ form_start(form) }}
    <ul class="formules" data-prototype="{{ form_widget(form.formules.vars.prototype)|e('html_attr') }}">
        {% for formule in form.formules %}
            <li>{{ form_row(formule.title) }}</li>
        {% endfor %}
    </ul>

    {#<button class="btn">{{ button_label|default('Save') }}</button>#}
{{ form_end(form) }}